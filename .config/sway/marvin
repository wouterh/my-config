# Config specific for marvin
input type:touchpad {
	natural_scroll enabled
}

# set $extMon HDMI-A-1
set $extMon DP-3

output eDP-1 position 0,0
output $extMon position 1920,0

workspace $ws1 output eDP-1
workspace $ws2 output eDP-1
workspace $ws3 output $extMon
workspace $ws4 output $extMon
workspace $ws5 output $extMon
workspace $ws6 output $extMon
workspace $ws7 output $extMon
workspace $ws8 output $extMon
workspace $ws9 output eDP-1
workspace $ws10 output $extMon

exec /usr/bin/shikane
